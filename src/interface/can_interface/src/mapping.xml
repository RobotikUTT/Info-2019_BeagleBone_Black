<?xml version="1.0"?>
<!-- Example for future automatic can to msg mapping -->
<mapping>
	<!-- Input -->
	<input frame="ORDER_ORDER_COMPLETED" msg="StmDone" topic="/STM/Done"></input>

	<input frame="ORDER_CURRENT_POS" msg="Point" topic="/STM/Position">
		<param name="pos_x" size="2" pos="0" />
		<param name="pos_y" size="2" pos="1" />
		<param name="angle" size="2" pos="2" />
	</input>

	<!-- Outputs -->
	<output frame="ORDER_MANAGEMENT" msg="StmMode" topic="/STM/AsserManagement">
		<param name="value" size="1" pos="0" />
	</output>

	<output frame="ORDER_GOTOA" msg="Point" topic="/STM/GoToAngle">
		<param name="pos_x" size="2" pos="0" />
		<param name="pos_y" size="2" pos="0" />
		<param name="angle" size="2" pos="0" />
		<param name="direction" size="1" pos="0" />
	</output>

	<!-- TODO -->
</mapping>