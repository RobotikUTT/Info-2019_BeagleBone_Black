<launch>
	<param name="robot_description" command="$(find xacro)/xacro.py '$(find robot_description)/urdf/robot.xacro'" />  
	<include file="$(find gazebo_ros)/launch/empty_world.launch">	    
		<arg name="world_name" value="$(find robot_gazebo)/worlds/table.world" />	    
		<arg name="gui" value="true" />
	</include>

	<node name="robot_spawn" pkg="gazebo_ros" type="spawn_model" output="screen"
		args="-urdf -param robot_description -model robot" />
</launch>