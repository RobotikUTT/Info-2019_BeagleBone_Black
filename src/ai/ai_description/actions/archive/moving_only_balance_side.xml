<?xml version="1.0"?>
<!-- Example action tree-->
<group type="ordered">
	<!-- Go out of the zone -->
	<reach native="true">
		<!-- margin of 180 mm + 10 mm -->
		<arg name="x">1315</arg>
		<arg name="y">225</arg>
	</reach>

	<!-- Go behind things -->
	<reach native="true">
		<!-- margin of 180 mm + 10 mm -->
		<arg name="x">1315</arg>
		<arg name="y">1228</arg>
	</reach>
	<reach native="true">
		<!-- margin of 180 mm + 10 mm -->
		<arg name="x">1050</arg>
		<arg name="y">1400</arg>
	</reach>

	<!-- Go back home -->
	<reach native="true" points="9">
		<!-- margin of 180 mm + 10 mm -->
		<arg name="x">1050</arg>
		<arg name="y">200</arg>
	</reach>

</group>